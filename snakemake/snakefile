#ruleorder: phenotype

rule phenotype:
    input:
        issue_file_path = raw_data_dir + "ukb_gp_scripts.txt"
    output:
        output_path = data_dir + "temp/01_warf_df.txt"
    run:
        shell(sh_dir + "01_phenotype.sh {input.issue_file_path} {output.output_path}")